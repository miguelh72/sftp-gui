{
  "name": "sftp-gui",
  "version": "0.1.0",
  "description": "SFTP GUI that wraps the system's OpenSSH sftp binary",
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "package": "electron-builder --config electron-builder.yml && powershell -Command \"Compress-Archive -Force -Path dist\\win-unpacked\\* -DestinationPath dist\\sftp-gui-portable.zip\"",
    "typecheck": "tsc --noEmit -p tsconfig.node.json && tsc --noEmit -p tsconfig.web.json"
  },
  "keywords": [
    "sftp",
    "ssh",
    "file-transfer",
    "electron",
    "openssh",
    "post-quantum"
  ],
  "author": "Miguel Hernandez",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/miguelh72/sftp-gui.git"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller",
      "esbuild"
    ],
    "overrides": {
      "node-gyp": "12.2.0"
    }
  },
  "dependencies": {
    "framer-motion": "12.34.3",
    "lucide-react": "0.575.0",
    "node-pty": "1.1.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "ssh-config": "5.1.0",
    "strip-ansi": "7.2.0",
    "uuid": "13.0.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/vite": "4.2.1",
    "@types/node": "25.3.3",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/uuid": "11.0.0",
    "@vitejs/plugin-react": "4.7.0",
    "electron": "40.6.1",
    "electron-builder": "26.8.1",
    "electron-vite": "4.0.1",
    "tailwindcss": "4.2.1",
    "typescript": "5.9.3",
    "vite": "6.4.1"
  }
}
